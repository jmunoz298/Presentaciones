/*********************************************
 * GLOBAL STYLES
 *********************************************/
body {
  background: #F0F1EB;
  background-color: #F0F1EB; }

.reveal {
open_sansregula
  font-family: 'open_sansregular', sans-serif, serif;
  font-size: 32px;
  font-weight: normal;
  color: #000; }

::selection {
  color: #fff;
  background: #26351C;
  text-shadow: none; }

.reveal .slides > section, .reveal .slides > section > section {
  line-height: 1.3;
  font-weight: inherit;
  /*text-align: left;*/}

.reveal .slides {
  margin-top: 2px;
 }

/*********************************************
 * HEADERS
 *********************************************/
.reveal h1, .reveal h2, .reveal h3, .reveal h4, .reveal h5, .reveal h6 {
  margin: 0px 0 1em 0;
  padding: 5px 10px 5px 10px;
  color: #383D3D;
  font-family: 'Quicksand', sans-serif;
  font-weight: normal;
  line-height: 1.2;
  letter-spacing: normal;
  text-transform: none;
  text-shadow: none;
  text-align: left;
  word-wrap: break-word; }


 .reveal h1 { 
  font-size: 1.5em;
  text-align: left;
  background: black;
  color: white;
  /*background: whitesmoke;
  background: ivory;
  background: mintcream;*/
  background-clip: padding-box;}

/* .reveal h2 {
  font-size: 2em; }
*/

.reveal h2 {
    padding: 20px 10px 10px 0;
    margin: 20px 0 25px 0;
    letter-spacing: 0.02em;
    font-size: 1.5em;
    text-align: center;
    /* color: #c00; */
    border-bottom: 3px solid #ccc;
}
.reveal h3 {
  font-size: 1.55em; }

.reveal h4 {
  font-size: 1.2em;
  text-align: center;}

.reveal .title{
  font-size: 2.5em;
  text-align: center;
  background: none;
  color: black;}



/*********************************************
 * IMAGES
 *********************************************/
/*** CAMBIO JUAN ***/
.reveal img {
  margin: 15px;
  border: .1em solid #fff;
  -webkit-box-shadow: 0px 0px 40px -2px rgba(0,0,0,0.75);
  -moz-box-shadow: 0px 0px 40px -2px rgba(0,0,0,0.75);
  box-shadow: 0px 0px 40px -2px rgba(0,0,0,0.75);
}

html.laback .reveal section img {
  border: none;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}

