/*********************************************
 * OTHER
 *********************************************/
.reveal p {
  margin: 20px 0;
  line-height: 1.3; 
  text-align: left;}

.reveal figcaption {
  color: #686868;
  font-size: .7em;
}

/* Ensure certain elements are never larger than the slide itself */
/*.reveal img, .reveal video, .reveal iframe{
  max-width: 75%;
  max-height: 75%; } */



.reveal strong, .reveal b {
  font-weight: bold; }

.reveal em {
  font-style: italic; }


.reveal ol, .reveal dl, .reveal ul {
  display: inline-block;
  text-align: left;
  margin: 0 0 0 1em; 
 line-height: 1.5em; 
  }

.reveal dl {
  background-color: white; 
}


.reveal ol {
  list-style-type: decimal; }

.reveal ul {
  list-style-type: disc; }

.reveal ul ul {
  list-style-type: square; }

.reveal ul ul ul {
  list-style-type: circle; }

.reveal ul ul, .reveal ul ol, .reveal ol ol, .reveal ol ul {
  display: block;
  margin-left: 40px; }

.reveal dt {
  font-weight: bold; 
  background-color: cornflowerblue;
  padding-left: 10px;}

.reveal dd {
  margin-left: 40px;
  margin-right: 20px; }

.reveal ul {
  background-color: rgba(255,255,255, 0.7);
  padding: 1em;
  list-style: circle;
  list-style-image: url("../../img/square-rounded-24.png");
  min-width: 60%;
  }

.reveal li{
	background-color: rgba(165,165,255,0.3);
	margin: .3em;
	padding: 0 0.5em 0 0.5em;
}

.reveal q, .reveal blockquote {
  quotes: none; }

.reveal blockquote{
  display:block;
  background-color: rgba(255,255,255, 0.6);
  padding: 15px 15px 15px 85px;
  margin: 0px 0 20px;
  position: relative;
  
  /*Font*/
  /*font-family: Georgia, serif; */
  font-size: 1em;
  line-height: 1.5;
  color: rgb(0,0,0);
  text-align: justify;
  
  /*Borders - (Optional)*/
  /*border-left: 15px solid blue; /*#c76c0c; */
  /*border-right: 2px solid blue; /*#c76c0c; */
  /* border-radius: 15px; */
  
  /*Box Shadow - (Optional)*/
  -moz-box-shadow: 2px 2px 15px #ccc;
  -webkit-box-shadow: 2px 2px 15px #ccc;
  box-shadow: 2px 2px 15px #ccc;
}

.reveal blockquote::before{
  content: "\201C"; /*Unicode for Left Double Quote*/
  
  /*Font*/
  font-family: Georgia, serif;
  font-size: 120px;
  font-weight: bold;
  color: red;
  
  /*Positioning*/
  position: absolute;
  left: 10px;
  top:-25px;
}

.reveal blockquote::after{
  /*Reset to make sure*/
  content: "";
}

.reveal blockquote a{
  text-decoration: none;
  background: #eee;
  cursor: pointer;
  padding: 0 3px;
  color: #c76c0c;
}

.reveal blockquote a:hover{
 color: #666;
}

.reveal blockquote em{
  font-style: italic;
}

.reveal q {
  font-style: italic; }

.reveal pre {
  display: block;
  position: relative;
  width: 90%;
  margin: 20px auto;
  text-align: left;
  font-size: 0.55em;
  font-family: monospace;
  line-height: 1.2em;
  word-wrap: break-word;
  box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.3); }

.reveal code {
  /* font-family: monospace; } */
  font-size: 0.75em;
  text-align: left;}

.reveal pre code {

  padding: 5px;
  overflow: hid;
  max-height: 400px;
  /* word-wrap: normal; */
  background: #3F3F3F;
  color: #DCDCDC; }



.reveal table {
  margin: auto;
  border-collapse: collapse;
  border-spacing: 0; }

.reveal table th {
  font-weight: bold; }

.reveal table th, .reveal table td {
  text-align: left;
  padding: 0.2em 0.5em 0.2em 0.5em;
  border-bottom: 1px solid; }

/*
.reveal table tr:last-child td {
  border-bottom: none; }
*/

.reveal sup {
  vertical-align: super; }

.reveal sub {
  vertical-align: sub; }

.reveal small {
  display: inline-block;
  font-size: 0.6em;
  line-height: 1.2em;
  vertical-align: top; }

.reveal small * {
  vertical-align: top; }

/*********************************************
 * LINKS
 *********************************************/
.reveal a {
  color: #51483D;
  text-decoration: none;
  -webkit-transition: color 0.15s ease;
  -moz-transition: color 0.15s ease;
  transition: color 0.15s ease; }

.reveal a:hover {
  color: #8b7b69;
  text-shadow: none;
  border: none; }

.reveal .roll span:after {
  color: #fff;
  background: #25211c; }



/*********************************************
 * NAVIGATION CONTROLS
 *********************************************/
.reveal .controls div.navigate-left, .reveal .controls div.navigate-left.enabled {
  border-right-color: #3b759e; }

.reveal .controls div.navigate-right, .reveal .controls div.navigate-right.enabled {
  border-left-color: #3b759e; }

.reveal .controls div.navigate-up, .reveal .controls div.navigate-up.enabled {
  border-bottom-color: #3b759e; }

.reveal .controls div.navigate-down, .reveal .controls div.navigate-down.enabled {
  border-top-color: #3b759e; }

.reveal .controls div.navigate-left.enabled:hover {
  border-right-color: #74a7cb; }

.reveal .controls div.navigate-right.enabled:hover {
  border-left-color: #74a7cb; }

.reveal .controls div.navigate-up.enabled:hover {
  border-bottom-color: #74a7cb; }

.reveal .controls div.navigate-down.enabled:hover {
  border-top-color: #74a7cb; }

/*********************************************
 * PROGRESS BAR
 *********************************************/
.reveal .progress {
  background: rgba(0, 0, 0, 0.2); }

.reveal .progress span {
  background: #51483D;
  -webkit-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  -moz-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985); }

/*********************************************
 * SLIDE NUMBER
 *********************************************/
.reveal .slide-number {
  color: #51483D; }

.reveal footer .footer {
  position: absolute; 
  bottom: 0px;
  margin-left: 0px;
  width: 100%;
  background-color:#fff;
    filter:alpha(opacity=5); 
    opacity:0.5; 
  border-color:red;}




.fondo .reveal figcaption, .fondo .reveal code{
  display: block; /*-webkit-inline-box; */
  background-color: black; 
  opacity: 0.7;
  color: white;
  font-weight: bold;
  padding: 10px;}

.fondo {
 background-color: rgba(0, 0, 0, 0.6);}

.reveal  .mitit{
font-size: 2em;
text-align: center;
border-bottom: 0;
background-color: rgba(165,165,255,0.3);}

h1:empty {
background: none;}

.reveal table img{
  margin: 0px;
  border: 0px solid black;
  -webkit-box-shadow: none;
  box-shadow: none; }

