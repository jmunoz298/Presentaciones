@article{Beauvoisprescriptivepowertelevision2012,
  title = {The Prescriptive Power of the Television Host. {{A}} Transposition of {{Milgram}}'s Obedience Paradigm to the Context of {{TV}} Game Show},
  volume = {62},
  issn = {11629088},
  abstract = {Introduction. \textendash{} Today's fascination with television makes us wonder whether it might not represent an
 authority capable of leading people in a television studio to inflict cruel acts on others, even though they
 condemn those acts.
Objective. \textendash{} The experiment reported here allows us to answer this question in the affirmative. Therefore,
 we transposed Milgram's famous experimental obedience paradigm to the context of a ``real'' TV game
 show, in the studio of a large television production company, with a live audience and no prizes.
Method. \textendash{} We set up several experimental conditions designed to tell us if, in such contexts, obedience
 was the dominant response, as it is in the often-replicated classic situation. We also wished to know if
 the introduction of variations would reduce obedience.
Results. \textendash{} The results show that obedience to the host is the dominant response, as it is in Milgram's classic
 situation. However, variations that are assumed to reduce this obedience do not in fact demonstrate the
 expected effects. An additional experimental condition appears to demonstrate that a determining factor
 of obedience is the physical proximity of the host incarnating the televisual power.
Conclusion. \textendash{} We offer a conclusion addressing the societal aspects of obedience.},
  number = {3},
  journal = {Revue Europ{\'e}enne de Psychologie Appliqu{\'e}e/European Review of Applied Psychology},
  doi = {10.1016/j.erap.2012.02.001},
  author = {Beauvois, Jean-L{\'e}on and Courbet, Didier and Oberl{\'e}, Dominique},
  month = jul,
  year = {2012},
  keywords = {Tengo: Electrónico,Diapos: Obediencia,Obedience to authority},
  pages = {111-119},
  file = {/home/jmunoz/Textos Html/Influencia/ThePrescriptivePowerOfTheTelevision.pdf}
}

