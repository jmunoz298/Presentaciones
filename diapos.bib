@article{passini_authority_2009,
	title = {Authority relationships between obedience and disobedience.},
	volume = {27},
	issn = {0732-118X},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2008-17890-007〈=es&site=ehost-live},
	doi = {10.1016/j.newideapsych.2008.06.001},
	abstract = {Recent episodes of public dissent (such as the demonstrations against G8 policies) raise the issue of the psychological processes triggered in obeying and disobeying the authority. Even if obedience to authority is an important aspect of social life and it plays a key role in maintaining social order, the concept of obedience has been studied in social psychology mainly in terms of its destructive aspects. Besides, most of the studies have overlooked the role of disobedience in the authority relationship. Disobedience may be conceived of as a protest that undermines the legitimacy of the authority or it can represent an instrument for controlling the legitimacy of the authority’s demands, becoming a factor protecting against authoritarianism. In this article, a new perspective on the study of the relationship between the individual and the authority is put forward, considering obedience and disobedience as parallel concepts, each having constructive and destructive aspects. (PsycINFO Database Record (c) 2010 APA, all rights reserved) (journal abstract)},
	number = {1},
	journal = {New Ideas in Psychology},
	author = {Passini, Stefano and Morselli, Davide},
	month = apr,
	year = {2009},
	keywords = {Tengo: Electrónico, Diapos: Obediencia, Authority, disobedience, Obedience to authority, defiance},
	pages = {96--106},
